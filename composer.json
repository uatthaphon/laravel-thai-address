{
    "name": "uatthaphon/laravel-thai-address",
    "description": "Thai provincial database with latitude and longitude.",
    "keywords": ["laravel", "thai address"],
    "license": "MIT",
    "authors": [
        {
            "name": "Atthaphon Urairat",
            "email": "u.atthaphon@gmail.com",
            "homepage": "https://atthaphon.urairat.me/"
        }
    ],
    "require": {
        "php": ">=5.6.4"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.7.*"
    },
    "autoload": {
        "classmap": [
            "src/",
            "src/database/migrations",
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Uatthaphon\\ThaiAddress\\": "src/",
            "Uatthaphon\\ThaiAddress\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uatthaphon\\ThaiAddress\\ThaiAddressServiceProvider"
            ]
        }
    }
}
